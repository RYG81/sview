<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="StCloneDesktop" />
		<Option pch_mode="2" />
		<Option compiler="windows_sdk_x86" />
		<Build>
			<Target title="WIN_gcc_x86">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-mmmx" />
					<Add option="-msse" />
					<Add option="-DUNICODE" />
					<Add option="-D_WIN32_WINNT=0x0500" />
					<Add option="-D__ST_DEBUG__" />
				</Compiler>
				<Linker>
					<Add library="gdi32" />
					<Add library="user32" />
					<Add library="kernel32" />
					<Add library="Version" />
					<Add library="nvapi" />
				</Linker>
			</Target>
			<Target title="WIN_vc_x86">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="windows_sdk_x86" />
				<Compiler>
					<Add option="/MD" />
					<Add option="/Ox" />
					<Add option="/W4" />
					<Add option="/EHsc" />
					<Add option="/MP" />
					<Add option="/DUNICODE" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="/NODEFAULTLIB:libcmt.lib" />
					<Add option="/MANIFEST" />
					<Add library="gdi32" />
					<Add library="user32" />
					<Add library="kernel32" />
					<Add library="Shell32" />
					<Add library="Advapi32" />
					<Add library="nvapi" />
				</Linker>
				<ExtraCommands>
					<Add after='mt.exe /nologo /manifest &quot;$(TARGET_OUTPUT_FILE).manifest&quot; /outputresource:&quot;$(TARGET_OUTPUT_FILE)&quot;;1' />
				</ExtraCommands>
			</Target>
			<Target title="WIN_vc_x86_DEBUG">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="windows_sdk_x86" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/Od" />
					<Add option="/W4" />
					<Add option="/Zi /D_DEBUG" />
					<Add option="/Zi" />
					<Add option="/EHsc" />
					<Add option="/MP" />
					<Add option="/DUNICODE" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/DNDEBUG" />
					<Add option="/D__ST_DEBUG__" />
				</Compiler>
				<Linker>
					<Add option="/DEBUG" />
					<Add option="/NODEFAULTLIB:libcmt.lib" />
					<Add option="/MANIFEST" />
					<Add library="gdi32" />
					<Add library="user32" />
					<Add library="kernel32" />
					<Add library="Shell32" />
					<Add library="Advapi32" />
					<Add library="Version" />
					<Add library="nvapi" />
				</Linker>
				<ExtraCommands>
					<Add after='mt.exe /nologo /manifest &quot;$(TARGET_OUTPUT_FILE).manifest&quot; /outputresource:&quot;$(TARGET_OUTPUT_FILE)&quot;;1' />
				</ExtraCommands>
			</Target>
			<Target title="WIN_vc_AMD64">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="windows_sdk_x86_64" />
				<Compiler>
					<Add option="/MD" />
					<Add option="/Ox" />
					<Add option="/W4" />
					<Add option="/EHsc" />
					<Add option="/MP" />
					<Add option="/DUNICODE" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="/NODEFAULTLIB:libcmt.lib" />
					<Add option="/MANIFEST" />
					<Add library="gdi32" />
					<Add library="user32" />
					<Add library="kernel32" />
					<Add library="Shell32" />
					<Add library="Advapi32" />
					<Add library="nvapi64" />
				</Linker>
				<ExtraCommands>
					<Add after='mt.exe /nologo /manifest &quot;$(TARGET_OUTPUT_FILE).manifest&quot; /outputresource:&quot;$(TARGET_OUTPUT_FILE)&quot;;1' />
				</ExtraCommands>
			</Target>
			<Target title="LINUX_gcc">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-mmmx" />
					<Add option="-msse" />
					<Add option="`pkg-config gtk+-2.0 --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`pkg-config gtk+-2.0 --libs`" />
					<Add library="Xrandr" />
					<Add library="X11" />
				</Linker>
			</Target>
			<Target title="LINUX_gcc_DEBUG">
				<Option output="../bin/$(TARGET_NAME)/StCloneDesktop" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/$(TARGET_NAME)/" />
				<Option object_output="obj/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-mmmx" />
					<Add option="-msse" />
					<Add option="`pkg-config gtk+-2.0 --cflags`" />
					<Add option="-D__ST_DEBUG__" />
				</Compiler>
				<Linker>
					<Add option="`pkg-config gtk+-2.0 --libs`" />
					<Add library="Xrandr" />
					<Add library="X11" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="../3rdparty/include" />
			<Add directory="../include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="../include" />
		</ResourceCompiler>
		<Linker>
			<Add library="StShared" />
			<Add directory="../3rdparty/lib/$(TARGET_NAME)" />
			<Add directory="../lib/$(TARGET_NAME)" />
		</Linker>
