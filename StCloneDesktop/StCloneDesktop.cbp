		<Unit filename="../StCore/StADLsdk.h" />
		<Unit filename="../StCore/StEDIDParser.cpp" />
		<Unit filename="../StCore/StEDIDParser.h" />
		<Unit filename="../StCore/StSearchMonitors.cpp" />
		<Unit filename="../StCore/StSearchMonitors.h" />
		<Unit filename="StCloneDesktop.cpp" />
		<Unit filename="StCloneDesktop.h" />
		<Unit filename="StCloneDesktop.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
